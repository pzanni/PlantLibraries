<p id="notice"><%= notice %></p>

<h1>Listing Plantclubs</h1>

<div class="row">
  <div class="col-md-4 col-md-offset-0">

    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
        </tr>
      </thead>

      <tbody>
        <% @plantclubs.each do |plantclub| %>
          <tr>
            <td><%= link_to plantclub.name, plantclub %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>
  </div>
</div>


