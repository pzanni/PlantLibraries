
<h1> <%= @user.username %> </h1>

<table class:="table table-hover">
	<tr>
		<th>Plantlibrary</th>
		<th>Plant Club</th>
	</tr>
	<tbody>
		<tr>
			<td><%= link_to @user.plantlibrary.name, @user.plantlibrary %></td>
			<td><%= link_to @user.plantclub.name, @user.plantclub %></td>
		</tr>
	</tbody>
</table>

<% if current_user == @user %>
	<%= link_to 'Edit', edit_user_path(@user) %> |
<% end %>
